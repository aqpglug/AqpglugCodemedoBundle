{% extends 'AqpglugCodemedoBundle:Admin:layout.html.twig' %}

{% block title %}{{ 'new'|trans|title }} {{ type|title }}{% endblock %}

{% block admin %}
<form class="block" action="{{ form_action }}" method="post" {{ form_enctype(form) }}>
    <div class="buttons">
        <input type="submit" value="{{ 'save'|trans|title }}" />
    </div>
    {{ form_widget(form) }}
</form>
{% endblock %}