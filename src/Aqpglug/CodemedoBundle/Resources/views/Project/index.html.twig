{% extends 'AqpglugCodemedoBundle::layout.html.twig' %}
{% block title %} Proyectos {% endblock %} 
{% block content %}
    <h1>Proyectos</h1>
    <ul class="projects">
    {% for project in projects%}
        <li>
            <a href="{{ project.metadata.url }}" target="_blank">{{ project.title }}</a>
            <img src="/bundles/aqpglugcodemedo/images/{{ project.image|default('default_project.jpg')}}" alt="{{ project.title }}" />
        </li>
    {% endfor %}
    </ul>
{% endblock %}